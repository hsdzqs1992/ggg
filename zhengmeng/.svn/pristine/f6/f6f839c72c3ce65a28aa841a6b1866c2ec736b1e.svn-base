<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhuye.zhengmeng">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ThemeBase">
        <activity
            android:name=".activity.lanuch.SplashActivity"
            android:theme="@style/ThemeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 引导页 -->
        <activity
            android:name=".activity.lanuch.GuideActivity"
            android:theme="@style/ThemeGuide" />
        <activity android:name=".MainActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".activity.register.RegisterActivity" />
        <activity android:name=".activity.register.ForgetActivity" />
        <activity android:name=".activity.register.ForgetActivity2" />
    </application>

</manifest>